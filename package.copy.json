{
  "name": "tuner",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.8",
    "@types/uuid": "^8.3.1",
    "animate.css": "^4.1.1",
    "express": "^4.17.1",
    "idb": "^6.1.2",
    "normalize.css": "^8.0.1",
    "pitchy": "^2.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-notifications-component": "^4.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^5.0.1",
    "react-select": "^5.8.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "uuid": "^3.4.0",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "node server/index.js",
    "dev": "set NODE_OPTIONS=--openssl-legacy-provider && react-scripts start",
    "devs": "set NODE_OPTIONS=--openssl-legacy-provider && set HTTPS=true&&set SSL_CRT_FILE=./.cert/cert.pem && set SSL_KEY_FILE=./.cert/key.pem && npm run dev",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "heroku-postbuild": "npm run build",
    "start:server": "node build_server/index.js",
    "dev:server": "nodemon server/index.ts",
    "build:server": "tsc --p ./server/tsconfig.server.json",
    "prod": "docker-compose -f docker-compose.prod.yaml up -d",
    "prod-rebuild": "docker-compose -f docker-compose.prod.yaml up -d --no-deps --build app",
    "lint": "eslint",
    "ts": "tsc --extendedDiagnostics --noEmit"
  },
  "browserslist": {
    "production": [">0.2%", "not dead", "not op_mini all"],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/eslint": "^8.56.10",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.23",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "nodemon": "^2.0.15",
    "redux-devtools-extension": "^2.13.9",
    "sass": "^1.32.8",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  }
}
